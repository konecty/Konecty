	"scripts": {
		"clean": "rimraf dist",
		"build:app": "webpack --mode production",
		"build:server": "webpack --config webpack.server.config.js --mode production",
		"build": "run-s clean build:*",
		"start:app": "webpack serve --config webpack.config.js --mode development",
		"start:server": "webpack --config webpack.server.config.js --watch --mode development | pino-pretty",
		"start": "env-cmd run-p start:*",
		"format": "prettier --write \"src/**/*.+(js|jsx|json|css|md)\"",
		"lint": "eslint src/",
		"lint:fix": "eslint --fix src/"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx}": [
			"eslint",
			"pretty-quick â€” staged",
			"git add"
		]
	},
